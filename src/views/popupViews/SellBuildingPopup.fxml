<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.popupControllers.SellBuildingPopupController">
   <children>
      <VBox prefHeight="400.0" prefWidth="300.0">
         <children>
            <Label prefHeight="20.0" prefWidth="77.0" text="Select City">
               <VBox.margin>
                  <Insets left="115.0" right="55.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="cityComboBox" prefHeight="26.0" prefWidth="152.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="78.0" right="20.0" top="20.0" />
               </VBox.margin>
            </ComboBox>
            <Button mnemonicParsing="false" onAction="#showBuildingNumber" prefHeight="25.0" prefWidth="148.0" text="Show Building Number">
               <VBox.margin>
                  <Insets bottom="20.0" left="80.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="buildingNumberLabel" alignment="TOP_LEFT" prefHeight="39.0" prefWidth="282.0" text="Label">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label prefHeight="20.0" prefWidth="134.0" text="Write Number to Sell">
               <VBox.margin>
                  <Insets left="85.0" right="25.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField>
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="sellButton" mnemonicParsing="false" onAction="#sellBuildings" text="Sell">
               <VBox.margin>
                  <Insets bottom="10.0" left="135.0" right="65.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="resultLabel" alignment="TOP_LEFT" prefHeight="26.0" prefWidth="310.0" text="Label">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#closeButtonClicked" text="Close">
               <VBox.margin>
                  <Insets bottom="10.0" left="130.0" right="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
