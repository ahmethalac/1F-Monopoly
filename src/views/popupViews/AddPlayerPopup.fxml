<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="250.0" minWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.popupControllers.AddPlayerPopupController">
    <children>
        <VBox alignment="CENTER" layoutX="131.0" layoutY="50.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add Player">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="25.0" top="25.0" />
               </VBox.margin></Text>
                <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="10.0">
                    <children>
                        <Label alignment="CENTER_RIGHT" minWidth="100.0" text="Name">
                     <font>
                        <Font size="18.0" />
                     </font></Label>
                        <TextField fx:id="nameField" alignment="CENTER" onAction="#onEnter" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></TextField>
                    </children>
                    <padding>
                        <Insets right="30.0" />
                    </padding>
                </HBox>
                <HBox alignment="CENTER_RIGHT">
                    <children>
                  <Button mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></Button>
                        <Button mnemonicParsing="false" onAction="#addButtonClicked" text="Add" />
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <padding>
                        <Insets right="30.0" />
                    </padding>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
                </HBox>
            <VBox fx:id="addPlayerVBox" prefHeight="200.0" prefWidth="100.0" />
            </children>
        </VBox>
    </children>
</AnchorPane>
