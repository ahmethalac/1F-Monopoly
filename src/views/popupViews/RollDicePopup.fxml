<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.popupControllers.RollDicePopupController">
   <children>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="56.0" prefHeight="175.0" prefWidth="276.0">
         <children>
            <ImageView fx:id="imageView1" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </ImageView>
            <ImageView fx:id="imageView2" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
      <Button layoutX="128.0" layoutY="292.0" mnemonicParsing="false" onAction="#closeButtonClicked" text="Close" />
   </children>
</AnchorPane>
