<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="358.0" prefWidth="196.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.popupControllers.BuyBuildingPopupController">
   <children>
      <VBox prefHeight="400.0" prefWidth="300.0">
         <children>
            <Label prefHeight="20.0" prefWidth="71.0" text="Select City">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="110.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="comboBox" onAction="#comboBoxUpdated" prefHeight="26.0" prefWidth="198.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="50.0" right="20.0" top="20.0" />
               </VBox.margin>
            </ComboBox>
            <Label fx:id="comboBoxLabel" alignment="TOP_LEFT" prefHeight="64.0" prefWidth="280.0" text="Label">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
            </Label>
            <Label prefHeight="17.0" prefWidth="180.0" text="Enter number of buildings to buy">
               <VBox.margin>
                  <Insets bottom="10.0" left="59.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="textField">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#buyBuildings" prefHeight="25.0" prefWidth="50.0" text="Buy">
               <VBox.margin>
                  <Insets bottom="10.0" left="125.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="resultLabel" alignment="TOP_LEFT" prefHeight="44.0" prefWidth="260.0" text="Label">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#closeButtonClicked" text="Close">
               <VBox.margin>
                  <Insets bottom="20.0" left="124.0" right="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
