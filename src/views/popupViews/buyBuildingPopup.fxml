<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="358.0" prefWidth="196.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.popupControllers.buyBuildingPopupController">
   <children>
      <VBox prefHeight="382.0" prefWidth="218.0">
         <children>
            <Label text="Select City and Building">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="comboBox" prefWidth="150.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </ComboBox>
            <HBox prefHeight="40.0" prefWidth="268.0">
               <children>
                  <Button fx:id="oneHouseButton" mnemonicParsing="false" onAction="#oneHouse" prefHeight="26.0" prefWidth="66.0" text="1 House">
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="twoHouseButton" mnemonicParsing="false" onAction="#twoHouses" prefHeight="26.0" prefWidth="66.0" text="2 Houses">
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="57.0" prefWidth="218.0">
               <children>
                  <Button fx:id="threeHouseButton" mnemonicParsing="false" onAction="#threeHouses" prefHeight="26.0" prefWidth="66.0" text="3 Houses">
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="fourHouseButton" mnemonicParsing="false" onAction="#fourHouses" text="4 Houses">
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Button fx:id="hotelButton" mnemonicParsing="false" onAction="#hotel" prefHeight="26.0" prefWidth="66.0" text="Hotel">
                     <HBox.margin>
                        <Insets bottom="10.0" left="72.0" right="20.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Label fx:id="comboBoxLabel" prefHeight="106.0" prefWidth="178.0" text="Label">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
