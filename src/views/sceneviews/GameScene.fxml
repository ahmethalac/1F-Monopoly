<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.scenecontrollers.GameSceneController">
   <children>
      <BorderPane pickOnBounds="false">
   
         <top>
            <Button text="top" />
         </top>
         <bottom>
            <Button text="bottom" />
         </bottom>
         <left>
            <Button text="left" />
         </left>
         <right>
            <Button onAction="#rotateTable" text="right" />
         </right>
         <center>
            <Button mnemonicParsing="false" BorderPane.alignment="CENTER" text="Test city button" onAction="#handleCityPopup"/>
         </center>
      </BorderPane>
   </children>
</StackPane>
