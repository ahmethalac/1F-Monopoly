<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="controllers.scenecontrollers.GameSceneController">
   <BorderPane pickOnBounds="false">
      <top>
         <Button text="top"/>
      </top>
      <bottom>
         <Button text="bottom"/>
      </bottom>
      <left>
         <Button text="left"/>
      </left>
      <right>
         <Button onAction="#rotateTable" text="right"/>
      </right>
   </BorderPane>
</StackPane>
